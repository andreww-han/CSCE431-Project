<p id="notice"><%= notice %></p>

<h1>Member Points</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Points</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @member_points.each do |member_point| %>
      <tr>
        <td><%= member_point.date %></td>
        <td><%= member_point.points %></td>
        <td><%= link_to 'Show', member_point %></td>
        <td><%= link_to 'Edit', edit_member_point_path(member_point) %></td>
        <td><%= link_to 'Destroy', member_point, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Member Point', new_member_point_path %>
