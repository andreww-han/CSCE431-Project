<div class='wrapper'>
  <%= render(:partial => 'navbar', :locals => {:is_logged_in => true}) %>
    <div class='container'>
       <% if @current_member.isAdmin or (@member.uid == @current_member.uid) %>

          <h1>Editing Member</h1>

          <%= render 'form', member: @member, current_member: @current_member %>

          <%= link_to 'Show', @member %> |
          <%= link_to 'Back', members_path %>
      <% else %>
        <% redirect_to members_path %>
      <% end %>
    </div>
</div>